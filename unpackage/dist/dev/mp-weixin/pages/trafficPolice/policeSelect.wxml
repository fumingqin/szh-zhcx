<view><map class="map vue-ref" style="{{'height:'+(mapHeight)+';'}}" id="map1" scale="{{scale}}" longitude="{{longitude}}" latitude="{{latitude}}" polyline="{{polyline}}" markers="{{markers}}" show-location="{{true}}" data-ref="map1"></map><image style="width:120rpx;height:120rpx;position:fixed;top:50px;left:1px;" src="../../static/trafficPolice/back.png" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view style="position:fixed;bottom:160rpx;"><view style="flex-direction:row;background-color:#FFFFFF;width:698rpx;border-radius:20px;padding:10rpx;align-items:center;margin-left:28rpx;"><view><image style="width:84rpx;height:84rpx;" src="../../static/trafficPolice/HeadPhoto.png"></image></view><view style="margin-left:10rpx;"><view><text style="font-size:34rpx;font-family:Source Han Sans SC;font-weight:bold;color:#2C2D2D;">{{driverName}}</text></view><view style="flex-direction:row;margin-top:10rpx;"><view><text style="font-size:28rpx;font-family:Source Han Sans SC;font-weight:300;color:#2C2D2D;">{{vehicleNumber}}</text></view><view style="margin-left:18rpx;"><text style="font-size:28rpx;font-family:Source Han Sans SC;font-weight:300;color:#2C2D2D;">豪华SUV</text></view></view></view><view style="margin-left:240rpx;"><image style="width:60rpx;height:60rpx;" src="../../static/trafficPolice/Phone.png"></image></view></view><view style="width:698rpx;height:216rpx;background-color:#FFFFFF;margin-left:28rpx;border-radius:20rpx;margin-top:20rpx;"><view><text style="height:40rpx;font-size:36rpx;font-family:Source Han Sans SC;font-weight:500;color:#333333;line-height:36rpx;margin-top:32rpx;margin-left:36rpx;">行程信息</text></view><view style="margin-top:24rpx;margin-left:38rpx;display:flex;flex-direction:row;"><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">起点：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{startSiteName}}</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;margin-left:40rpx;">终点：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{endSiteName}}</text></view><view style="margin-top:20rpx;margin-left:36rpx;display:flex;flex-direction:row;"><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">人数：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{people+"人"}}</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;margin-left:40rpx;">上车时间：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{time}}</text></view></view><view style="width:698rpx;height:160rpx;background-color:#FFFFFF;margin-left:28rpx;border-radius:20rpx;margin-top:20rpx;"><view><text style="height:40rpx;font-size:36rpx;font-family:Source Han Sans SC;font-weight:500;color:#333333;line-height:36rpx;margin-top:32rpx;margin-left:36rpx;">当前车辆位置</text></view><view style="margin-top:24rpx;margin-left:38rpx;display:flex;flex-direction:row;"><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">地址：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{address}}</text></view></view><view style="width:698rpx;height:216rpx;background-color:#FFFFFF;margin-left:28rpx;border-radius:20rpx;margin-top:20rpx;"><view><text style="height:40rpx;font-size:36rpx;font-family:Source Han Sans SC;font-weight:500;color:#333333;line-height:36rpx;margin-top:32rpx;margin-left:36rpx;">志愿者信息</text></view><view style="margin-top:24rpx;margin-left:38rpx;display:flex;flex-direction:row;"><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">姓名：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{volunteerName}}</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;margin-left:150rpx;">性别：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{sex}}</text></view><view style="margin-top:20rpx;margin-left:36rpx;display:flex;flex-direction:row;"><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">手机号：</text><text style="font-size:30rpx;font-family:Source Han Sans SC;color:#666666;line-height:36rpx;">{{passengerPhone}}</text></view></view><view><button data-event-opts="{{[['longpress',[['confirmgetonCar',['$event']]]]]}}" style="width:698rpx;height:105rpx;background-color:#0073CE;border-color:#0073CE;color:#FFFFFF;margin-left:28rpx;border-radius:20rpx;position:fixed;padding:6rpx;margin-top:20rpx;" bindlongpress="__e"><text style="color:#FFFFFF;">拨打志愿者手机</text></button></view></view></view>